<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>com.nekoimi.nk</groupId>
    <artifactId>nk-cloud-framework</artifactId>
    <packaging>pom</packaging>
    <name>root</name>
    <description>nk-cloud-framework-dependencies</description>
    <modules>
        <module>nk-framework</module>
        <module>nk-gateway</module>
        <module>nk-services</module>
        <module>nk-devtools</module>
        <module>nk-auth</module>
        <module>nk-platform</module>
    </modules>
    <version>1.0</version>

    <properties>
        <nk.version>${version}</nk.version>
        <java.version>11</java.version>
        <maven.compiler.source>11</maven.compiler.source>
        <maven.compiler.target>11</maven.compiler.target>
        <lombok.version>1.18.20</lombok.version>
        <spring-boot.version>2.3.7.RELEASE</spring-boot.version>
        <spring-cloud.version>Hoxton.SR9</spring-cloud.version>
        <spring-cloud-alibaba.version>2.2.2.RELEASE</spring-cloud-alibaba.version>
        <apache-commons-lang3.version>3.12.0</apache-commons-lang3.version>
        <apache-commons-codec.version>1.15</apache-commons-codec.version>
        <apache-commons-io.version>2.10.0</apache-commons-io.version>
        <mybatis-plus.version>3.4.1</mybatis-plus.version>
        <maven-resources-plugin.version>3.1.0</maven-resources-plugin.version>
        <maven-surefire-plugin.version>2.22.2</maven-surefire-plugin.version>
    </properties>

    <!--环境-->
    <profiles>
        <profile>
            <id>dev</id>
            <activation>
                <!--默认激活-->
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <!--具体环境变量配置-->
                <debug>false</debug>
                <profile.name>dev</profile.name>
                <!--数据库环境配置-->
                <db.driver-class-name>com.p6spy.engine.spy.P6SpyDriver</db.driver-class-name>
                <db.url>jdbc:p6spy:mysql://localhost:3306/nk_cloud?useUnicode=true&amp;characterEncoding=utf8&amp;useSSL=false&amp;allowPublicKeyRetrieval=true</db.url>
                <db.username>root</db.username>
                <db.password>123456</db.password>
                <db.redis.host>localhost</db.redis.host>
                <db.redis.port>6379</db.redis.port>
                <db.redis.password>123456</db.redis.password>
                <db.redis.database>0</db.redis.database>
            </properties>
        </profile>
        <profile>
            <id>test</id>
            <activation>
                <!--默认激活-->
                <activeByDefault>false</activeByDefault>
            </activation>
            <properties>
                <!--具体环境变量配置-->
                <debug>false</debug>
                <profile.name>test</profile.name>
                <!--数据库环境配置-->
                <db.driver-class-name>com.mysql.cj.jdbc.Driver</db.driver-class-name>
                <db.url>jdbc:mysql://localhost:3306/nk_cloud?useUnicode=true&amp;characterEncoding=utf8&amp;useSSL=false&amp;allowPublicKeyRetrieval=true</db.url>
                <db.username>root</db.username>
                <db.password>123456</db.password>
                <db.redis.host>localhost</db.redis.host>
                <db.redis.port>6379</db.redis.port>
                <db.redis.password>123456</db.redis.password>
                <db.redis.database>0</db.redis.database>
            </properties>
        </profile>
    </profiles>

    <developers>
        <developer>
            <name>nekoimi</name>
            <email>nekoimime@gmail.com</email>
            <url>https://nekoimi.com</url>
        </developer>
    </developers>

    <licenses>
        <license>
            <name>APACHE LICENSE, VERSION 2.0</name>
            <url>https://www.apache.org/licenses/LICENSE-2.0</url>
        </license>
    </licenses>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-dependencies</artifactId>
                <version>${spring-boot.version}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
            <dependency>
                <groupId>org.springframework.cloud</groupId>
                <artifactId>spring-cloud-dependencies</artifactId>
                <version>${spring-cloud.version}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
            <dependency>
                <groupId>com.alibaba.cloud</groupId>
                <artifactId>spring-cloud-alibaba-dependencies</artifactId>
                <version>${spring-cloud-alibaba.version}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
            <dependency>
                <groupId>org.projectlombok</groupId>
                <artifactId>lombok</artifactId>
                <version>${lombok.version}</version>
            </dependency>
            <dependency>
                <groupId>org.apache.commons</groupId>
                <artifactId>commons-lang3</artifactId>
                <version>${apache-commons-lang3.version}</version>
            </dependency>
            <dependency>
                <groupId>commons-codec</groupId>
                <artifactId>commons-codec</artifactId>
                <version>${apache-commons-codec.version}</version>
            </dependency>
            <dependency>
                <groupId>commons-io</groupId>
                <artifactId>commons-io</artifactId>
                <version>${apache-commons-io.version}</version>
            </dependency>
            <dependency>
                <groupId>com.baomidou</groupId>
                <artifactId>mybatis-plus-boot-starter</artifactId>
                <version>${mybatis-plus.version}</version>
            </dependency>
            <dependency>
                <groupId>com.baomidou</groupId>
                <artifactId>mybatis-plus-extension</artifactId>
                <version>${mybatis-plus.version}</version>
            </dependency>
            <dependency>
                <groupId>com.nekoimi.nk</groupId>
                <artifactId>nk-core</artifactId>
                <version>${nk.version}</version>
            </dependency>
            <dependency>
                <groupId>com.nekoimi.nk</groupId>
                <artifactId>nk-web</artifactId>
                <version>${nk.version}</version>
            </dependency>
            <dependency>
                <groupId>com.nekoimi.nk</groupId>
                <artifactId>nk-security</artifactId>
                <version>${nk.version}</version>
            </dependency>
            <dependency>
                <groupId>com.nekoimi.nk</groupId>
                <artifactId>nk-mybatis</artifactId>
                <version>${nk.version}</version>
            </dependency>
            <dependency>
                <groupId>com.nekoimi.nk</groupId>
                <artifactId>nk-redis</artifactId>
                <version>${nk.version}</version>
            </dependency>
        </dependencies>
    </dependencyManagement>

    <dependencies>
        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
        </dependency>
    </dependencies>

    <repositories>
        <repository>
            <id>aliyun-repos</id>
            <url>https://maven.aliyun.com/repository/public</url>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>
    </repositories>
    <pluginRepositories>
        <pluginRepository>
            <id>aliyun-plugin</id>
            <url>https://maven.aliyun.com/repository/spring-plugin</url>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </pluginRepository>
    </pluginRepositories>
</project>